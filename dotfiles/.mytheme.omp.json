{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " \uf07c {{ .Path }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffe97d",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#4b95e9",
          "type": "git",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
